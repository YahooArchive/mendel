{
  "name": "mendel",
  "version": "1.0.6",
  "description": "Build toolchain for experimentation on isomorphic web applications with tree-inheritance and multivariate support.",
  "keywords": [
    "testing",
    "a/b",
    "bucket",
    "multivariate",
    "experiments",
    "evaluation",
    "build-tools",
    "build"
  ],
  "author": "Irae Carvalho <irae@irae.pro.br>",
  "contributors": [
    {
      "name": "David Gomez",
      "email": "david.garcia.mx@gmail.com"
    },
    {
      "name": "Rafael Martins",
      "email": "snit.ram@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/mendel"
  },
  "main": "lib/trees.js",
  "bin": {
    "mendel": "bin/index.js"
  },
  "scripts": {
    "test": "npm run unit && npm run lint",
    "lint": "eslint .",
    "unit": "tap test/*.js",
    "unit-file": "tap",
    "linkall": "node ./bin/linkall.js",
    "coverage": "tap test/*.js --coverage --nyc-arg=--all",
    "coverage-file": "tap --coverage --coverage-report=lcov",
    "coverage-html": "tap test/*.js --coverage --nyc-arg=--all --coverage-report=lcov",
    "coverage-all-individualy": "for test in $(find test -name *.js -depth 1); do tap $test --coverage ; done"
  },
  "nyc": {
    "exclude": [
      "examples/**",
      "coverage/**",
      "test/**",
      "packages/*/lib/**"
    ]
  },
  "dependencies": {
    "async": "^1.5.2",
    "browserify": "^13.0.0",
    "mkdirp": "^0.5.1",
    "xtend": "^4.0.1",
    "mendel-config": "^1.0.5",
    "mendel-browserify": "^1.0.6",
    "mendel-development": "^1.0.6",
    "mendel-requirify": "^1.0.6"
  },
  "devDependencies": {
    "eslint": "^2.3.0",
    "eslint-cli": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "module": "^1.2.5",
    "rimraf": "^2.5.2",
    "tap": "^5.7.0",
    "temp": "^0.8.3",
    "through2": "^2.0.1"
  }
}
