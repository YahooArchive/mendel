{
  "name": "mendel-full-example",
  "version": "3.0.0",
  "description": "Example of app using mendel, es6, jsx, react, server-side-rendering, multivariant and multilayer support.",
  "main": "index.js",
  "scripts": {
    "test": "mendel-mocha --prelude 'test/setup.js' **/_test_/*.js",
    "test-dev": "mendel-mocha --prelude 'test/setup/*.js' **/_test_/*.js --watch",
    "coverage": "mendel-mocha --prelude 'test/setup/*.js' **/_test_/*.js --reporter=mocha-istanbul-reporter --reporter-options=text,text-summary",
    "build": "NODE_ENV=production mendel",
    "daemon": "mendel --watch",
    "development": "nodemon index.js",
    "production": "NODE_ENV=production node index.js"
  },
  "dependencies": {
    "express": "^4.13.4",
    "mendel-middleware": "*",
    "morgan": "^1.7.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-node-env-inline": "0.0.2",
    "babel-plugin-transform-react-remove-prop-types": "*",
    "babel-preset-babili": "0.0.8",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^3.5.0",
    "envify": "^3.4.1",
    "eslint-plugin-react": "^5.2.2",
    "jasmine-core": "^2.5.2",
    "jsdom": "^9.9.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-es6-shim": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-diff-reporter": "^1.1.1",
    "karma-mendel": "*",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "mendel-development-middleware": "*",
    "mendel-generator-extract": "*",
    "mendel-generator-node-modules": "*",
    "mendel-generator-prune": "*",
    "mendel-manifest-extract-bundles": "*",
    "mendel-manifest-uglify": "*",
    "mendel-mocha-runner": "*",
    "mendel-outlet-browser-pack": "*",
    "mendel-outlet-css": "*",
    "mendel-outlet-manifest": "*",
    "mendel-outlet-server-side-render": "*",
    "mendel-parser-json": "*",
    "mendel-pipeline": "*",
    "mendel-transform-babel": "*",
    "mendel-transform-less": "*",
    "mocha": "^2.5.3",
    "mocha-istanbul-reporter": "^0.1.0",
    "nodemon": "^1.11.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rtlcss": "^2.1.2",
    "watchify": "^3.9.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-inline-environment-variables"
    ],
    "retainLines": true
  },
  "contributors": [
    {
      "name": "Irae Carvalho",
      "email": "irae@irae.pro.br"
    },
    {
      "name": "David Gomez",
      "email": "david.garcia.mx@gmail.com"
    }
  ],
  "keywords": [
    "mendel",
    "app",
    "example"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/mendel"
  }
}
